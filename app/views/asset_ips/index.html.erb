<%= stylesheet_link_tag('targets') %>

<div class="row">
  <div class="col-xs-6 col-sm-6" id="logPanel">
  </div>
  <div class="col-xs-6 col-sm-6">
    <div class="pull-right col-sm-auto" style="margin:10px">
      <%= link_to raw('<li class="fa fa-plus"></li>添加网站'), new_target_asset_ip_path(@target), remote: true, class: "btn btn-info btn-sm"%>
    </div>
  </div>
</div>
<!-- Start .bredcrumb -->
<ul id="crumb" class="breadcrumb">
  <li><i class="im-home"></i><a href="/my">管理首页</a><i class="en-arrow-right7"></i></li>
  <li><i class="im-home"></i><%= link_to @target.name, target_path(@target)%><i class="en-arrow-right7"></i></li>
  <li><i class="im-screen"></i> 网站资产列表 </li>
</ul>
<!-- End .breadcrumb -->

<div class="ip-list-index">
<%= render 'index' %>
</div>


<div class="modal fade bs-modal-sm add-ip-modal" tabindex="-1" role="dialog" aria-hidden="true" id="add-ip-modal">
</div>

<script>
    $(document).ready(function() {
        refresh_assets_from_data('ipTree', <%=raw(@treedata.to_json)%>);
        <%#= render 'reload.js' %>
    });
</script>

<%= javascript_include_tag('targets') %>